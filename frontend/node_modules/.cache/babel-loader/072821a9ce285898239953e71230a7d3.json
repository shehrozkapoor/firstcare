{"ast":null,"code":"import { FilterMatchMode, FilterOperator } from 'primevue/api';\nexport default {\n  name: \"CustomDataTable\",\n  props: ['columns', 'data'],\n\n  data() {\n    return {\n      filters: {\n        'global': {\n          value: null,\n          matchMode: FilterMatchMode.STARTS_WITH\n        },\n        'id': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.STARTS_WITH\n          }]\n        },\n        'name': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.STARTS_WITH\n          }]\n        },\n        'code': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.STARTS_WITH\n          }]\n        },\n        'description': {\n          operator: FilterOperator.AND,\n          constraints: [{\n            value: null,\n            matchMode: FilterMatchMode.STARTS_WITH\n          }]\n        }\n      },\n      selectedRows: null,\n      loading: false,\n      columns: this.columns,\n      data: this.data\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuCA,SAAQA,eAAR,EAAwBC,cAAxB,QAA6C,cAA7C;AAEA,eAAe;AACXC,MAAI,EAAE,iBADK;AAEXC,OAAK,EAAC,CAAC,SAAD,EAAW,MAAX,CAFK;;AAGXC,MAAI,GAAG;AACH,WAAO;AACHC,aAAO,EAAE;AACL,kBAAU;AAACC,eAAK,EAAE,IAAR;AAAcC,mBAAS,EAAEP,eAAe,CAACQ;AAAzC,SADL;AAEL,cAAM;AAACC,kBAAQ,EAAER,cAAc,CAACS,GAA1B;AAA+BC,qBAAW,EAAE,CAAC;AAACL,iBAAK,EAAE,IAAR;AAAcC,qBAAS,EAAEP,eAAe,CAACQ;AAAzC,WAAD;AAA5C,SAFD;AAGL,gBAAQ;AAACC,kBAAQ,EAAER,cAAc,CAACS,GAA1B;AAA+BC,qBAAW,EAAE,CAAC;AAACL,iBAAK,EAAE,IAAR;AAAcC,qBAAS,EAAEP,eAAe,CAACQ;AAAzC,WAAD;AAA5C,SAHH;AAIL,gBAAQ;AAACC,kBAAQ,EAAER,cAAc,CAACS,GAA1B;AAA+BC,qBAAW,EAAE,CAAC;AAACL,iBAAK,EAAE,IAAR;AAAcC,qBAAS,EAAEP,eAAe,CAACQ;AAAzC,WAAD;AAA5C,SAJH;AAKL,uBAAe;AAACC,kBAAQ,EAAER,cAAc,CAACS,GAA1B;AAA+BC,qBAAW,EAAE,CAAC;AAACL,iBAAK,EAAE,IAAR;AAAcC,qBAAS,EAAEP,eAAe,CAACQ;AAAzC,WAAD;AAA5C;AALV,OADN;AAQHI,kBAAY,EAAE,IARX;AASHC,aAAO,EAAE,KATN;AAUHC,aAAO,EAAE,KAAKA,OAVX;AAWHV,UAAI,EAAE,KAAKA;AAXR,KAAP;AAaJ;;AAjBW,CAAf","names":["FilterMatchMode","FilterOperator","name","props","data","filters","value","matchMode","STARTS_WITH","operator","AND","constraints","selectedRows","loading","columns"],"sourceRoot":"","sources":["/Users/shehrozkapoor/Desktop/FIRST_CARE/firstcare-frontend/src/components/Custom/CustomDataTable.vue"],"sourcesContent":["<template>\n    <div>\n        <DataTable :value=\"data\" responsiveLayout=\"scroll\" stripedRows :paginator=\"true\" :rows=\"10\"\n            v-model:filters=\"filters\" filterDisplay=\"menu\" :loading=\"loading\"\n            v-model:selection=\"selectedRows\"\n            paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink CurrentPageReport RowsPerPageDropdown\"\n            :rowsPerPageOptions=\"[10,25,50]\"\n            currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords} entries\"\n            :globalFilterFields=\"['id','code','name','rating']\">\n            <template #header>\n                <div class=\"flex flex-column md:flex-row md:justify-content-between md:align-items-center\">\n                    <h5 class=\"m-0\">Patients</h5>\n                    <span class=\"p-input-icon-left\">\n                        <i class=\"pi pi-search\" />\n                        <InputText v-model=\"filters['global'].value\" placeholder=\"Keyword Search\" />\n                    </span>\n                </div>\n            </template>\n            <template #empty>\n                No customers found.\n            </template>\n            <template #loading>\n                Loading customers data. Please wait.\n            </template>\n            <Column selectionMode=\"multiple\" style=\"min-width: 3rem\"></Column>\n            <Column v-for=\"col of columns\" :field=\"col.field\" :header=\"col.header\" :key=\"col.field\" sortable style=\"min-width: 14rem\">\n                <template #body=\"{data}\">\n                    {{data.col.field}}\n                </template>\n                <template #filter=\"{filterModel}\">\n                    <InputText type=\"text\" v-model=\"filterModel.value\" class=\"p-column-filter\"\n                        placeholder=\"Search by Id\" />\n                </template>\n            </Column>\n        </DataTable>\n    </div>\n</template>\n\n<script>\nimport {FilterMatchMode,FilterOperator} from 'primevue/api';\n\nexport default {\n    name: \"CustomDataTable\",\n    props:['columns','data'],\n    data() {\n        return {\n            filters: {\n                'global': {value: null, matchMode: FilterMatchMode.STARTS_WITH},\n                'id': {operator: FilterOperator.AND, constraints: [{value: null, matchMode: FilterMatchMode.STARTS_WITH}]},\n                'name': {operator: FilterOperator.AND, constraints: [{value: null, matchMode: FilterMatchMode.STARTS_WITH}]},\n                'code': {operator: FilterOperator.AND, constraints: [{value: null, matchMode: FilterMatchMode.STARTS_WITH}]},\n                'description': {operator: FilterOperator.AND, constraints: [{value: null, matchMode: FilterMatchMode.STARTS_WITH}]},\n            },\n            selectedRows: null,\n            loading: false,\n            columns: this.columns,\n            data: this.data\n        }\n    }\n}\n</script>\n\n\n<style lang=\"scss\" scoped>\n::v-deep(.p-paginator) {\n    .p-paginator-current {\n        margin-left: auto;\n    }\n}\n\n::v-deep(.p-progressbar) {\n    height: .5rem;\n    background-color: #D8DADC;\n\n    .p-progressbar-value {\n        background-color: #607D8B;\n    }\n}\n\n::v-deep(.p-datepicker) {\n    min-width: 25rem;\n\n    td {\n        font-weight: 400;\n    }\n}\n\n::v-deep(.p-datatable.p-datatable-customers) {\n    .p-datatable-header {\n        padding: 1rem;\n        text-align: left;\n        font-size: 1.5rem;\n    }\n\n    .p-paginator {\n        padding: 1rem;\n    }\n\n    .p-datatable-thead > tr > th {\n        text-align: left;\n    }\n\n    .p-datatable-tbody > tr > td {\n        cursor: auto;\n    }\n\n    .p-dropdown-label:not(.p-placeholder) {\n        text-transform: uppercase;\n    }\n}\n</style>"]},"metadata":{},"sourceType":"module"}