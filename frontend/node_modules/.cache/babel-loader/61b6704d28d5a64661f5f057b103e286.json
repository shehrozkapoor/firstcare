{"ast":null,"code":"import { FilterMatchMode, FilterOperator } from 'primevue/api';\nexport default {\n  name: \"CustomDataTable\",\n  props: ['table_columns', 'table_data', 'title', 'multipleSelect', 'paginator', 'filter', 'sortable', 'show_header'],\n\n  data() {\n    return {\n      filters: {\n        'global': {\n          value: null,\n          matchMode: FilterMatchMode.STARTS_WITH\n        }\n      },\n      multiSelect: this.multipleSelect ?? false,\n      selectedRows: null,\n      globalFilterField: [],\n      loading: false,\n      columns: this.table_columns,\n      data: this.table_data\n    };\n  },\n\n  created() {\n    this.table_columns.forEach(item => this.filters[item.field] = {\n      operator: FilterOperator.AND,\n      constraints: [{\n        value: null,\n        matchMode: FilterMatchMode.STARTS_WITH\n      }]\n    });\n    this.table_columns.forEach(item => this.globalFilterField.push(item.field));\n  }\n\n};","map":{"version":3,"mappings":"AAuCA,SACIA,eADJ,EAEIC,cAFJ,QAGO,cAHP;AAKA,eAAe;AACXC,MAAI,EAAE,iBADK;AAEXC,OAAK,EAAE,CAAC,eAAD,EAAkB,YAAlB,EAA+B,OAA/B,EAAuC,gBAAvC,EAAwD,WAAxD,EAAoE,QAApE,EAA6E,UAA7E,EAAwF,aAAxF,CAFI;;AAGXC,MAAI,GAAG;AACH,WAAO;AACHC,aAAO,EAAE;AACL,kBAAU;AACNC,eAAK,EAAE,IADD;AAENC,mBAAS,EAAEP,eAAe,CAACQ;AAFrB;AADL,OADN;AAOHC,iBAAW,EAAC,KAAKC,cAAL,IAAqB,KAP9B;AAQHC,kBAAY,EAAE,IARX;AASHC,uBAAiB,EAAC,EATf;AAUHC,aAAO,EAAE,KAVN;AAWHC,aAAO,EAAE,KAAKC,aAXX;AAYHX,UAAI,EAAE,KAAKY;AAZR,KAAP;AAcH,GAlBU;;AAmBXC,SAAO,GAAG;AACN,SAAKF,aAAL,CAAmBG,OAAnB,CAA2BC,IAAG,IAC1B,KAAKd,OAAL,CAAac,IAAI,CAACC,KAAlB,IAA2B;AAC3BC,cAAQ,EAAEpB,cAAc,CAACqB,GADE;AAE3BC,iBAAW,EAAE,CAAC;AACVjB,aAAK,EAAE,IADG;AAEVC,iBAAS,EAAEP,eAAe,CAACQ;AAFjB,OAAD;AAFc,KAD/B;AAQA,SAAKO,aAAL,CAAmBG,OAAnB,CAA2BC,IAAG,IAAK,KAAKP,iBAAL,CAAuBY,IAAvB,CAA4BL,IAAI,CAACC,KAAjC,CAAnC;AACJ;;AA7BW,CAAf","names":["FilterMatchMode","FilterOperator","name","props","data","filters","value","matchMode","STARTS_WITH","multiSelect","multipleSelect","selectedRows","globalFilterField","loading","columns","table_columns","table_data","created","forEach","item","field","operator","AND","constraints","push"],"sourceRoot":"","sources":["/Users/shehrozkapoor/Desktop/FIRST_CARE/firstcare-frontend/src/components/Custom/CustomDataTable.vue"],"sourcesContent":["<template>\n    <div>\n        <DataTable :value=\"table_data\" responsiveLayout=\"scroll\" stripedRows :paginator=\"paginator??false\" :rows=\"10\"\n            v-model:filters=\"filters\" filterDisplay=\"menu\" :loading=\"loading\" v-model:selection=\"selectedRows\"\n            paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink CurrentPageReport RowsPerPageDropdown\"\n            :rowsPerPageOptions=\"[10,25,50]\"\n            currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords} entries\"\n            :globalFilterFields=\"globalFilterField\" :rowEditor=\"true\">\n            <template #header>\n                <div class=\"flex flex-column md:flex-row md:justify-content-between md:align-items-center\">\n                    <h5 class=\"m-0\">{{title}}</h5>\n                    <span class=\"p-input-icon-left\">\n                        <i class=\"pi pi-search\" />\n                        <InputText v-model=\"filters['global'].value\" placeholder=\"Keyword Search\" :v-show=\"show_header==true\"/>\n                    </span>\n                </div>\n            </template>\n            <template #empty>\n                No customers found.\n            </template>\n            <template #loading>\n                Loading customers data. Please wait.\n            </template>\n            <Column selectionMode=\"multiple\" style=\"min-width: 3rem\" v-if=\"multipleSelect\"></Column>\n            <Column v-for=\"col of columns\" :field=\"col.field\" :header=\"col.header\" :key=\"col.field\" :sortable=\"sortable??false\"\n                style=\"min-width: 14rem\">\n                <template #body=\"{data}\">\n                    {{data[col.field]}}\n                </template>\n                <template #filter=\"{filterModel}\" v-if=\"filter??false\">\n                    <InputText type=\"text\" v-model=\"filterModel.value\" class=\"p-column-filter\"\n                        placeholder=\"Search by Id\" />\n                </template>\n            </Column>\n        </DataTable>\n    </div>\n</template>\n\n<script>\nimport {\n    FilterMatchMode,\n    FilterOperator\n} from 'primevue/api';\n\nexport default {\n    name: \"CustomDataTable\",\n    props: ['table_columns', 'table_data','title','multipleSelect','paginator','filter','sortable','show_header'],\n    data() {\n        return {\n            filters: {\n                'global': {\n                    value: null,\n                    matchMode: FilterMatchMode.STARTS_WITH\n                },\n            },\n            multiSelect:this.multipleSelect??false,\n            selectedRows: null,\n            globalFilterField:[],\n            loading: false,\n            columns: this.table_columns,\n            data: this.table_data\n        }\n    },\n    created() {\n        this.table_columns.forEach(item =>\n            this.filters[item.field] = {\n            operator: FilterOperator.AND,\n            constraints: [{\n                value: null,\n                matchMode: FilterMatchMode.STARTS_WITH\n            }]\n        });\n        this.table_columns.forEach(item => this.globalFilterField.push(item.field))\n    }\n}\n</script>\n\n\n<style lang=\"scss\" scoped>\n::v-deep(.p-paginator) {\n    .p-paginator-current {\n        margin-left: auto;\n    }\n}\n\n::v-deep(.p-progressbar) {\n    height: .5rem;\n    background-color: #D8DADC;\n\n    .p-progressbar-value {\n        background-color: #607D8B;\n    }\n}\n\n::v-deep(.p-datepicker) {\n    min-width: 25rem;\n\n    td {\n        font-weight: 400;\n    }\n}\n\n::v-deep(.p-datatable.p-datatable-customers) {\n    .p-datatable-header {\n        padding: 1rem;\n        text-align: left;\n        font-size: 1.5rem;\n    }\n\n    .p-paginator {\n        padding: 1rem;\n    }\n\n    .p-datatable-thead > tr > th {\n        text-align: left;\n    }\n\n    .p-datatable-tbody > tr > td {\n        cursor: auto;\n    }\n\n    .p-dropdown-label:not(.p-placeholder) {\n        text-transform: uppercase;\n    }\n}\n</style>"]},"metadata":{},"sourceType":"module"}